<p id="notice"><%= notice %></p>
<h1>Users</h1>
<table>
  <thead>
  <tr>
    <th>Full name</th>
    <th>| email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.full_name %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>


